from flask import Flask, jsonify
import gdown
app = Flask(__name__)

@app.route('/run-colab')
def run_colab():
    gdown.download('https://colab.research.google.com/drive/1KYD9nMwvm4ogTb_u5urTJQJItRgAyuXK', 'FinRL_Ensemble_StockTrading_ICAIF_2020.ipynb', quiet=False)
    return jsonify(message='colab notebook ran successfully')
